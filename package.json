{
  "name": "rad-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.2.0",
    "apollo-client-preset": "^1.0.8",
    "graphql": "^0.13.0",
    "graphql-tag": "^2.7.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "build:css": "postcss src/index.tailwind.css -o src/index.css",
    "watch:css": "postcss src/index.tailwind.css -o src/index.css -w",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "postcss-cli": "^5.0.0",
    "tailwindcss": "^0.4.1"
  }
}
